{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "SMSman Studio default capabilities",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://api.sms-man.com/**" }]
    },
    "http:allow-fetch",
    "clipboard-manager:allow-write-text",
    "store:allow-load",
    "store:allow-set",
    "store:allow-get",
    "store:allow-delete",
    "store:allow-save",
    "store:allow-clear"
  ]
}
